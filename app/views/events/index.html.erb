<p id="notice"><%= notice %></p>
<h1>Events</h1>
<h2>Future</h2>
<ul>
  <% @future_events.each do |event| %>
    <li><%= event.description %> on <%= event.updated_at.to_s(:short) %></li>
  <% end %>
</ul>
<h2>Past Events</h2>
<ul>
  <% @past_events.each do |event| %>
    <li> <%= event.description %> on <%= event.updated_at.to_s(:short) %> </li>
  <% end %>
</ul>
<br>
<%= link_to 'New Event', new_event_path %>
